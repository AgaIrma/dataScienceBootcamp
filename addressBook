class PersonalData:
    def __init__(self,firstName,lastName,company,job,email):
        self.firstName = firstName
        self.lastName = lastName
        self.company = company
        self.job = job
        self.email = email

annaLis = PersonalData("Anna", "Lis","Google","Data Analyst","anna_lis@gmail.com")
hannaLis = PersonalData("Hanna", "Lis","Google","Data Engineer","hanna_lis@gmail.com")
joannaLis = PersonalData("Joanna", "Lis","Amazon","ETL Developer","joanna_lis@gmail.com")
annaRis = PersonalData("Anna", "Ris","Google","Data Analyst","anna_lis@gmail.com")
hannaRis = PersonalData("Hanna", "Ris","Google","Data Engineer","hanna_lis@gmail.com")
joannaRis = PersonalData("Joanna", "Ris","Amazon","ETL Developer","joanna_lis@gmail.com")

peopleInAddressBook = (annaLis, hannaLis, joannaLis, annaRis,hannaRis,joannaRis)

for i in peopleInAddressBook:
    print(i.firstName+" "+ i.lastName+" "+i.email)

