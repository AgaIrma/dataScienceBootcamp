class PersonalData:
    def __init__(self,firstName,lastName,company,job,email):
        self.firstName = firstName
        self.lastName = lastName
        self.company = company
        self.job = job
        self.email = email

annaLis = PersonalData("Anna", "Lis","Google","Data Analyst","anna_lis@gmail.com")
print(annaLis)
hannaLis = PersonalData("Hanna", "Lis","Google","Data Engineer","hanna_lis@gmail.com")
print(hannaLis)